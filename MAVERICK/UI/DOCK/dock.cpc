    .parameters = "TYPE:4 CTN:1 BORD:0"
    .px = "%posx%"
    .py = "%posy%"
    .sx = "300"
    .sy = "100"
    .WindowColor = "000,000,000"
    .BackColor = "198,198,198"
    .TitleColor = "000,000,000"
    .ImgTitre = ""
    .Icon = ""
    @#dock_ref create/
end/ window


